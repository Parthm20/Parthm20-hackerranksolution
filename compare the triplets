#include<stdio.h>
int main()
{
    int a[3],b[3],c[3],d[3],i,j;
   int counta=0,countb=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    for(int i=0;i<3;i++)
        {
        if(a[i]>b[i])
            counta++;
        else if(b[i]>a[i])
            countb++;
    }
     printf("%d %d",counta,countb);
   
}
